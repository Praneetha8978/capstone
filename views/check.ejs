<!DOCTYPE html>
<html>
    <head>
        <style>
        .b:hover{
            background: rgb(200, 212, 212);
        }
        body{
            background-color: rgb(200, 212, 212);
        }
        .t{
            margin-top: 225px;
        }
        .a{
            margin-left: 1408px;
            font-size: 30px;
            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
            color: black;
            text-decoration: none
        }
        </style>
    </head>
    <body>
        <a href="/logout" class="a">LOGOUT</a>
        <center>
            <h1>WELCOME</h1>
            <table border="1" class="t">
                <tr>
                    <th>REG:NO</th>
                    <th>ATTENDANCE</th>
                    <th>NUMBER OF DAYS</th>
                    <th>REASON</th>
                    <th>ACCEPT</th>
                    <th>DELETE</th>
                </tr>
                <% responseArr.forEach(function(ele){
                    %>
                    <tr>
                        <td><%=ele.regno%></td>
                        <td><%=ele.attendance%></td>
                        <td><%=ele.number_of_days%></td>
                        <td><%=ele.reason%></td>
                        <td><a href="/checks/<%=ele.regno%>"><input type="button" value="ACCEPT" class="b"></a></td>
                        <td><a href="/checksd/<%=ele.regno%>"><input type="button" value="DELETE" class="b"></a></td>
                    </tr>
                <%})%>
            </table>
        </center>
    </body>
</html>